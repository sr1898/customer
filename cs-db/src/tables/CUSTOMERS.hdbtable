COLUMN TABLE  "CUSTOMERS" (
    "CUSTOMERID" INTEGER GENERATED BY DEFAULT AS IDENTITY ( NO CYCLE NO CACHE INCREMENT BY 1 MAXVALUE 10 ) NOT NULL COMMENT 'CUSTOMER ID',
    "FULLNAME" NVARCHAR(250) COMMENT 'FULL NAME',
    "COUNTRYCODE"  NVARCHAR(3) COMMENT 'COUNTR CODE',
    PRIMARY KEY ("CUSTOMERID")
    ) COMMENT 'CUSTOMERS LIST'